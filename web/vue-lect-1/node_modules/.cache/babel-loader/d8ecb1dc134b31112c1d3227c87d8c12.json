etPostsURL: (state)=>{\n        return state.url\n    },\n    getPosts: (state) =>{\n        return state.posts\n    },\n    getPostsByID:(state)=>(id)=>{\n        return state.posts.find(p =>{\n            return p.id = id\n        })\n    },\n    getErrors: (state) =>{\n        return state.errors\n    },\n    getLoaded: (state) =>{\n        return state.loaded\n    }\n}\n\nexport default {\n    namespaced: true,\n    state,\n    mutations,\n    actions,\n    getters,\n}"]}]}